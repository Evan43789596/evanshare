  <process id="simpleAutoAudit" name="简单自动审批" isExecutable="true">
    <startEvent id="startevent1" name="开始"></startEvent>
    <serviceTask id="strategicDecision" name="决策引擎"></serviceTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="strategicDecision"></sequenceFlow>
