    </sequenceFlow>
    <sequenceFlow id="flow4" name="${creditScore &lt;=100}" sourceRef="exclusivegateway1" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${creditScore <=100}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="结束"></endEvent>
    <sequenceFlow id="flow5" sourceRef="loanService" targetRef="endevent2"></sequenceFlow>
  </process>
